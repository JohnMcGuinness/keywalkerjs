{
  "env": {
    "browser": true,
    "amd": true
  },
  "rules": {
    "no-console": 0,
    "no-shadow": 0,
    "dot-notation": 0,
    "no-underscore-dangle": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-labels": 0,
    "no-constant-condition": 0,
    "no-return-assign": 0,
    "no-extend-native": 0,
    "semi": [2, "always"],
    "space-before-blocks": [2, "always"],
    "spaced-comment": [2, "always"],
    "indent": [2, "tab", { "SwitchCase": 1 } ],
    "quotes": 0
  },
  "globals": {
    "KeyEvent": true
  }
}
